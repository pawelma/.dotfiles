URxvt*transparent: false
URxvt*depth: 10
URxvt*tintColor: White
URxvt*saveLines: 10000
URxvt*font: xft:Source Code Pro:pixelsize=13
URxvt*boldFont: xft:Source Code Pro:bold:pixelsize=13
URxvt*font: xft:Source\ Code\ Pro\ Medium:pixelsize=13:antialias=true:hinting=true,xft:Source\ Code\ Pro\ Medium:pixelsize=13:antialias=true:hinting=true
URxvt*scrollBar: false
URxvt*scrollBar_right: true
! do not scroll with output
URxvt*scrollTtyOutput: false

! scroll in relation to buffer (with mouse scroll or Shift+Page Up)
URxvt*scrollWithBuffer: true

! scroll back to the bottom on keypress
URxvt*scrollTtyKeypress: true
! URxvt*scrollstyle: rxvt

URxvt*urgentOnBell: true

Xft.dpi:        96

! X11 colors pieced together by Dmitry Medvinsky.
! Based on Vim colorscheme "badwolf" pieced together by Steve Losh.

*background: #1c1b1a
*foreground: #DEDEDE
*cursorColor: #aeee00
*scrollColor: #45413b
*fadeColor: #ffffff
*fading: 20

! black
*color0: #141413
*color8: #000000
! red
*color1: #ff2c4b
*color9: #ff2c4b
! green
*color2: #aeee00
*color10: #aeee00
! yellow
*color3: #fade3e
*color11: #ffa724
! blue
*color4: #0a9dff
*color12: #0a9dff
! magenta
*color5: #ff9eb8
*color13: #ff9eb8
! cyan
*color6: #8cffba
*color14: #8cffba
! white
*color7: #d9cec3
*color15: #ffffff

!scheme: pnevma
*foreground: #f8f6f2
*background: #3A3636



! scheme: material-theme
*.foreground:   #c4c7d1
*.background:   #1e282d
*.fadeColor:    #80CBC4
*.cursorColor:  #c4c7d1

! black
*.color0:       #073642
*.color8:       #ff002b

! red
*.color1:       #eb606b
*.color9:       #eb606b

! green
*.color2:       #c3e88d
*.color10:      #c3e88d

! yellow
*.color3:       #f7eb95
*.color11:      #f7eb95

! blue
*.color4:       #80cbc4
*.color12:      #7dc6bf

! magenta
*.color5:       #ff2490
*.color13:      #6c71c4

! cyan
*.color6:       #aeddff
*.color14:      #35434d

! white
*.color7:       #ffffff
*.color15:      #ffffff

! color for links select (matcher ext)
*.colorUL: #4682B4

URxvt.keysym.Home: \033[1~
URxvt.keysym.KP_Home: \033[1~

URxvt.iso14755: False
# URxvt.iso14755: off
# URxvt.iso14755_52: off

# URxvt.perl-lib: ${HOME}/.urxvt/ext/perls
URxvt.perl-lib: /usr/lib/urxvt/perl

URxvt.perl-ext-common: default,matcher,clipboard

URxvt.clipboard.copycmd:  xsel --input  --clipboard
URxvt.clipboard.pastecmd: xsel --output --clipboard

URxvt.keysym.Control-C: perl:clipboard:copy
URxvt.keysym.Control-V: perl:clipboard:paste

# clipboard
# URxvt.keysym.C-V: perl:clipboard:paste
# URxvt.keysym.Shift-C-v: perl:clipboard:paste
# URxvt.keysym.C-C: perl:clipboard:copy
# URxvt.keysym.Shift-C-C: perl:clipboard:copy

# URxvt.keysym.C-C: perl:clipboard:copy
# URxvt.keysym.C-V: perl:clipboard:paste

# URxvt.keysym.Shift-Control-V: perl:clipboard:paste
# URxvt.keysym.Shift-Control-C: perl:clipboard:copy

# Matcher ext
URxvt.perl-ext-common: default,matcher
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1

URxvt.keysym.C-Delete: perl:matcher:last
URxvt.keysym.M-Delete: perl:matcher:list

# Tabbed
URxvt.tabbed.tabbar-fg: 13
URxvt.tabbed.tabbar-bg: 14
URxvt.tabbed.tab-fg: 255
URxvt.tabbed.tab-bg: 14

# fix vim abcd
URxvt.keysym.Control-Up: \033[1;5A
URxvt.keysym.Control-Down: \033[1;5B
URxvt.keysym.Control-Left: \033[1;5D
URxvt.keysym.Control-Right: \033[1;5C
